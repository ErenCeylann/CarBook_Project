@model List<ResultFooterAdressDto>
@foreach (var item in Model)
{
    <footer class="ftco-footer ftco-bg-dark ftco-section">
        <div class="container">
            <div class="row mb-5">
                <div class="col-md">
                    <div class="ftco-footer-widget mb-4">
                        <h2 class="ftco-heading-2"><a href="/Default/Index" class="logo">Car<span>book</span></a></h2>
                        <p>@item.Description</p>
                        <ul class="ftco-footer-social list-unstyled float-md-left float-lft mt-5">
                            <li class="ftco-animate"><a href="#"><span class="icon-twitter"></span></a></li>
                            <li class="ftco-animate"><a href="#"><span class="icon-facebook"></span></a></li>
                            <li class="ftco-animate"><a href="#"><span class="icon-instagram"></span></a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-md">
                    <div class="ftco-footer-widget mb-4 ml-md-5">
                        <h2 class="ftco-heading-2">Hızlı Linkler</h2>
                        <ul class="list-unstyled">
                            <li><a href="/About/Index" class="py-2 d-block">Hakkımızda</a></li>
                            <li><a href="/Car/Index" class="py-2 d-block">Hizmetler</a></li>
                            <li><a href="javascript:void(0)" id="btn1" class="py-2 d-block">Şartlar & Koşullar</a></li>
                            <li><a href="javascript:void(0)" id="btn2" class="py-2 d-block">Güvenlik</a></li>
                            <li><a href="javascript:void(0)" id="btn3" class="py-2 d-block">Çerez Politikası</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-md">
                    <div class="ftco-footer-widget mb-4">
                        <h2 class="ftco-heading-2">Müşteri Desteği</h2>
                        <ul class="list-unstyled">
                            <li><a href="javascript:void(0)" id="btn4" class="py-2 d-block">Sıkça Sorulan Sorular</a></li>
                            <li><a href="javascript:void(0)" id="btn5" class="py-2 d-block">Ödeme Yöntemleri</a></li>
                            <li><a href="/CarPricing/Index" class="py-2 d-block">Kiralama</a></li>
                            <li><a href="javascript:void(0)" id="btn6" class="py-2 d-block">Nasıl Çalışır</a></li>
                            <li><a href="/Contact/Index" class="py-2 d-block">İletişim</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-md">
                    <div class="ftco-footer-widget mb-4">
                        <h2 class="ftco-heading-2">Bir sorun mu var ?</h2>
                        <div class="block-23 mb-3">
                            <ul>
                                <li><span class="icon icon-map-marker"></span><span class="text">203 Fake St. Mountain View, San Francisco, California, USA</span></li>
                                <li><a href="#"><span class="icon icon-phone"></span><span class="text">@item.Phone</span></a></li>
                                <li><a href="#"><span class="icon icon-envelope"></span><span class="text">@item.Email</span></a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12 text-center">

                    <p>
                        <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                        Copyright &copy;<script>document.write(new Date().getFullYear());</script> Tüm Hakları Saklıdır <i class="icon-heart color-danger" aria-hidden="true"></i> by <a href="https://colorlib.com" target="_blank">Colorlib</a>
                        <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                    </p>
                </div>
            </div>
        </div>
    </footer>
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script>
        $('#btn1').click(function(){
                    swal("Şartlar & Koşullar", "BookCar, kullanıcıların çevrim içi olarak araç kiralama işlemlerini gerçekleştirebileceği bir platformdur. Sisteme kayıt olan ve hizmetleri kullanan tüm kullanıcılar bu şartları peşinen kabul etmiş sayılır. BookCar üzerinden sunulan hizmet yalnızca kişisel kullanım içindir ve ticari amaçlarla kullanılamaz.Kullanıcılar, üyelik oluştururken doğru ve eksiksiz bilgi vermekle yükümlüdür. Hatalı ya da yanıltıcı bilgi verilmesi durumunda doğabilecek tüm sonuçlardan kullanıcı sorumludur. Kiralanan araç, sadece sistemde belirtilen kişi ya da kişiler tarafından kullanılabilir. Araç teslim alındıktan sonra oluşabilecek hasar, trafik cezası, gecikme, kayıp ya da benzeri tüm durumlarda sorumluluk tamamen kullanıcıya aittir. Aracın zamanında ve eksiksiz şekilde teslim edilmesi zorunludur. Teslim süresi aşıldığında veya araç eksik şekilde teslim Kiralama işlemi, ödeme tamamlandığında geçerli olur. Ödeme yapılmayan rezervasyonlar geçerli sayılmaz. Kullanıcılar, rezervasyonu belirli bir süre öncesinden iptal ettiklerinde ücret iadesi alabilir. Ancak son dakikada yapılan iptallerde kesinti uygulanabilir ya da ücret tamamen iade edilmeyebilir. BookCar, sistemin kesintiye uğraması, teknik sorunlar, araç bulunamaması ya da üçüncü kişilerden kaynaklı aksaklıklar sebebiyle yaşanabilecek gecikmelerden veya mağduriyetlerden sorumlu tutulamaz. Kullanıcıların kişisel verileri, yasal mevzuata uygun şekilde korunur ve gizlilik esasına göre işlenir. Ancak kullanıcı, hizmeti kullanarak verilerinin sistem içinde kullanılmasını kabul eder. BookCar, şartları ve koşulları dilediği zaman güncelleme hakkını saklı tutar. Güncellenen şartlar, sitede yayımlandığı andan itibaren geçerli olur. Kullanıcılar bu tür değişiklikleri takip etmekle yükümlüdür. Herhangi bir anlaşmazlık durumunda Türkiye Cumhuriyeti yasaları geçerlidir. Destek veya iletişim için BookCar yönetimiyle resmi kanallar üzerinden irtibata geçebilirsiniz.");
        });
        $('#btn2').click(function(){
                            swal("Güvenlik","BookCar olarak kullanıcılarımızın güvenliği bizim için önceliklidir. Tüm kullanıcı bilgileri, gizliliğe ve veri güvenliğine uygun olarak saklanmakta ve üçüncü kişilerle paylaşılmamaktadır. Sistemimize kaydolurken girdiğiniz ad, soyad, e-posta ve şifre gibi kişisel bilgiler şifrelenmiş biçimde korunur. Şifreler, sistem yöneticileri dahil hiç kimse tarafından görüntülenemez. Web sitemiz ve tüm işlemler, güvenli bağlantı (SSL) protokolü ile korunur. Bu sayede, kullanıcı ile sistem arasındaki tüm veri akışı şifrelenir ve kötü niyetli kişiler tarafından ele geçirilmesi engellenir. Kullanıcıların ödeme sırasında girdiği bilgiler, üçüncü taraf güvenli ödeme sistemleri aracılığıyla işlenir ve BookCar tarafından kaydedilmez. Hesabınızın güvenliğini sağlamak için güçlü ve tahmin edilmesi zor şifreler belirlemeniz önemlidir. Hesap bilgilerinizin güvenliği sizin sorumluluğunuzdadır. Hesabınızda şüpheli bir hareket fark ederseniz, vakit kaybetmeden bizimle iletişime geçmeniz önerilir. Kötü niyetli girişimlere karşı sistemimiz düzenli olarak güvenlik testlerinden geçirilmekte ve yazılım altyapısı güncel tutulmaktadır. Herhangi bir güvenlik ihlali tespit edildiğinde, en kısa sürede müdahale edilerek gerekli önlemler alınmaktadır. BookCar platformunu kullanan herkesin güvenli bir deneyim yaşaması için hem sistemsel hem de kullanıcı düzeyinde gerekli tedbirler alınmakta, kullanıcı güvenliği titizlikle sağlanmaktadır.");
        });

        $('#btn3').click(function(){
                                    swal("Çerezler","BookCar web sitesi, kullanıcı deneyimini iyileştirmek ve hizmetlerimizi daha verimli hale getirmek amacıyla çerezler kullanmaktadır. Bu sayfayı ziyaret ederek, çerez kullanımını kabul etmiş olursunuz.Çerezler, ziyaret ettiğiniz internet siteleri tarafından tarayıcınıza bırakılan küçük metin dosyalarıdır. Bu dosyalar, site içinde gezinirken sizi tanımamıza yardımcı olur ve tercihlerinizi hatırlayarak size daha kişiselleştirilmiş bir deneyim sunar. BookCar, site trafiğini analiz etmek, hataları tespit etmek, oturum bilgilerini yönetmek ve kullanıcı tercihlerini hatırlamak amacıyla çerezlerden faydalanır. Çerezler aracılığıyla elde edilen bilgiler, hiçbir şekilde kimliğinizi belirlemek amacıyla kullanılmaz ve kişisel verilerinizle birleştirilmez. Ayrıca üçüncü şahıslarla paylaşılmaz. Çerezler sadece sistemin daha doğru ve hızlı çalışmasını sağlamak için kullanılır. Dilerseniz çerez kullanımını tarayıcınızın ayarlarından engelleyebilir veya mevcut çerezleri silebilirsiniz. Ancak çerezleri devre dışı bırakmanız durumunda, BookCar web sitesinin bazı bölümleri düzgün çalışmayabilir ve kullanıcı deneyiminiz olumsuz etkilenebilir. BookCar, çerez politikası üzerinde değişiklik yapma hakkını saklı tutar. Güncellenen çerez politikası sitede yayımlandığı andan itibaren geçerli olur. Çerezlerle ilgili daha fazla bilgi almak isterseniz bizimle iletişime geçebilirsiniz.");
        });

        $('#btn4').click(function(){
                 swal("Sıkça Sorulan Sorular","Sorularınızı İletişim Sayfasından Sorabilirsiniz");
        });

        $('#btn5').click(function(){
                 swal("Ödeme Yöntemleri","Ödeme İşlemleri belirli lokasyonlarda sadece kart ile yapılmaktadır.");
        });

        $('#btn6').click(function(){
                 swal("Nasıl Çalışır","Maile gelen bilgileri kontrol edin ve aracınızı alın.");
        });
    </script>
}

